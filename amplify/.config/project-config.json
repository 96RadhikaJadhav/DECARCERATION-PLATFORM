{
    "providers": [
        "awscloudformation"
    ],
    "projectName": "webapp",
    "version": "3.0",
    "frontend": "javascript",
    "javascript": {
        "framework": "react",
        "config": {
            "SourceDir": "web-app/src",
            "DistributionDir": "web-app/src/dist",
            "BuildCommand": "cd web-app && npm run-script build",
            "StartCommand": "cd web-app && npm run-script start"
        }
    }
}